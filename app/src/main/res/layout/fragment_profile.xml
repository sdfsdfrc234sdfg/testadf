<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#f0f3f4"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".ui.ProfileFragment">


        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name"
            app:srcCompat="@drawable/ic_place_black_24dp" />

        <com.heart.tower.util.TopCropImageView
            android:id="@+id/icon_img"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/form_back"
            android:elevation="3dp"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/round"
            android:elevation="5dp"
            android:padding="20dp"
            android:src="@drawable/ic_message_black_24dp"
            app:layout_constraintBottom_toTopOf="@+id/place"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name" />

        <TextView
            android:id="@+id/place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="TextView"
            android:textColor="@color/text_color"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintStart_toEndOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7" />


        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textColor="@android:color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_img" />

        <TextView
            android:id="@+id/search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="@string/i_search"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" />

        <TextView
            android:id="@+id/getToKnow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/i_want_to_meet"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/search"
            app:layout_constraintTop_toBottomOf="@+id/search_text" />

        <TextView
            android:id="@+id/purposeOfDating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/purpose_of_dating"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/getToKnow"
            app:layout_constraintTop_toBottomOf="@+id/getToKnow_text" />

        <TextView
            android:id="@+id/my_details"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/my_details"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/purposeOfDating"
            app:layout_constraintTop_toBottomOf="@+id/purposeOfDating_text" />

        <TextView
            android:id="@+id/bodyType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/bodyType"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/my_details"
            app:layout_constraintTop_toBottomOf="@+id/myDetailsText" />

        <TextView
            android:id="@+id/education"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/education"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/appearance"
            app:layout_constraintTop_toBottomOf="@+id/appearance_text" />

        <TextView
            android:id="@+id/knowledgeOfLanguages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/knowledgeOfLanguages"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/education"
            app:layout_constraintTop_toBottomOf="@+id/education_text" />

        <TextView
            android:id="@+id/attitudeTowardsSmoking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/attitudeTowardsSmoking"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/orientation"
            app:layout_constraintTop_toBottomOf="@+id/orientation_text" />

        <TextView
            android:id="@+id/money_state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/money_state"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/attitudeToAlcohol"
            app:layout_constraintTop_toBottomOf="@+id/attitudeToAlcohol_text" />

        <TextView
            android:id="@+id/live_state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/Live"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/money_state"
            app:layout_constraintTop_toBottomOf="@+id/money_state_text" />

        <TextView
            android:id="@+id/children"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/Children"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/live_state"
            app:layout_constraintTop_toBottomOf="@+id/live_state_text" />

        <TextView
            android:id="@+id/attitudeToAlcohol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/attitudeToAlcohol"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/attitudeTowardsSmoking"
            app:layout_constraintTop_toBottomOf="@+id/attitudeTowardsSmoking_text" />

        <TextView
            android:id="@+id/orientation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/orientation"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/knowledgeOfLanguages"
            app:layout_constraintTop_toBottomOf="@+id/knowledgeOfLanguages_text" />

        <TextView
            android:id="@+id/appearance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/appearance"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/bodyType"
            app:layout_constraintTop_toBottomOf="@+id/bodyType_text" />

        <TextView
            android:id="@+id/search_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:paddingStart="10dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/search"
            app:layout_constraintTop_toBottomOf="@+id/search" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/getToKnow_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/getToKnow"
            app:layout_constraintTop_toBottomOf="@+id/getToKnow" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/knowledgeOfLanguages_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/knowledgeOfLanguages"
            app:layout_constraintTop_toBottomOf="@+id/knowledgeOfLanguages" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/orientation_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/orientation"
            app:layout_constraintTop_toBottomOf="@+id/orientation" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/attitudeTowardsSmoking_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/attitudeTowardsSmoking"
            app:layout_constraintTop_toBottomOf="@+id/attitudeTowardsSmoking" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/attitudeToAlcohol_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="200dp"
            android:textColor="@color/text_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/attitudeToAlcohol"
            app:layout_constraintTop_toBottomOf="@+id/attitudeToAlcohol" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/money_state_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/attitudeToAlcohol"
            app:layout_constraintTop_toBottomOf="@+id/money_state" />

        <TextView
            android:id="@+id/live_state_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:paddingStart="10dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/attitudeToAlcohol"
            app:layout_constraintTop_toBottomOf="@+id/live_state" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/shildren_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/attitudeToAlcohol"
            app:layout_constraintTop_toBottomOf="@+id/children" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/education_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/education"
            app:layout_constraintTop_toBottomOf="@+id/education" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/appearance_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/appearance"
            app:layout_constraintTop_toBottomOf="@+id/appearance" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/bodyType_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/bodyType"
            app:layout_constraintTop_toBottomOf="@+id/bodyType" />

        <TextView
            android:id="@+id/myDetailsText"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:paddingStart="10dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/my_details"
            app:layout_constraintTop_toBottomOf="@+id/my_details" />

        <TextView
            android:paddingStart="10dp"
            android:background="@drawable/edit_back"
            android:padding="3dp"
            android:id="@+id/purposeOfDating_text"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textColor="@color/text_color"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/purposeOfDating"
            app:layout_constraintTop_toBottomOf="@+id/purposeOfDating" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
